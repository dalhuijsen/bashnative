seq () { 
    local fr="$1"
    local to="$2"
    local step="--"
    local comp=">="
    local offs="+1"
    if (( fr < to )); then
        step="++"
        comp="<="
        offs="-1"
    fi
    while ((  fr${step} ${comp} $to )); do
        echo $((fr ${offs}))
    done
}
