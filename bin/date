main () {
	X=0
	while (( X++ < ${#} )); do
		if [[ ${!X:0:1} == "+" ]]; then #starts with a plus outformat is set
			OUTFORMAT="${!X:1}"
		fi
	done
	
	printf "%(${OUTFORMAT:-"%x %X"})T" ${INPUTTIME:-"-1"}
}


main "$@"
